@model IEnumerable<OnlineStore.Models.IdentityModels.User>

    <h2>List of users</h2>
    <table class="table">
        @foreach (var user in Model)
        {
            <tr>
                <td class="text-dark">@user.Email</td>
                <td>
                    <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userId="@user.Id">Permissions</a>
                </td>
            </tr>
        }
    </table>